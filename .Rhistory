library(rjson)
library(tidyverse)
library(purrr)
library(data.table)
library(lubridate)
# taking json file to an r list of lists
list <- fromJSON(file = "lookout.json")[[1]]
# turning list of lists into a data frame, selecting out columns that aren't useful
# formating start_date column with lubridate for graphing
lookout_leaderboard <- rbindlist(list) %>%
select(-moving_time, -start_date_local) %>%
mutate(start_date = ymd_hms(start_date))
lookout_leaderboard %>%
ggplot(aes(x = start_date, y = elapsed_time)) +
geom_point()
lookout_leaderboard %>%
ggplot(aes(x = start_date, y = elapsed_time, alpha = 0.5, fill = "orange", size = rank)) +
geom_point()
lookout_leaderboard %>%
ggplot(aes(x = start_date, y = elapsed_time, alpha = 0.5, color = "orange", size = rank)) +
geom_point()
lookout_leaderboard %>%
ggplot(aes(x = start_date, y = elapsed_time, alpha = 0.5, color = "orangered", size = rank)) +
geom_point()
lookout_leaderboard %>%
ggplot(aes(x = start_date, y = elapsed_time, alpha = 0.5, color = "orangered", size = rank)) +
scale_size_continuous(range = c(15000, 1))
geom_point()
lookout_leaderboard %>%
ggplot(aes(x = start_date, y = elapsed_time, alpha = 0.5, color = "orangered", size = rank)) +
scale_size_continuous(range = c(15000, 1)) +
geom_point()
lookout_leaderboard %>%
ggplot(aes(x = start_date, y = elapsed_time, alpha = 0.5, color = "orangered", size = rank)) +
scale_size_continuous(range = c(15000, 1)) +
geom_point()
lookout_leaderboard %>%
ggplot(aes(x = start_date, y = elapsed_time, alpha = 0.5, color = "orangered", size = rank)) +
scale_size_continuous(name = "rank", range = c(15000, 1)) +
geom_point()
lookout_leaderboard %>%
ggplot(aes(x = rank, y = elapsed_time, alpha = 0.5, color = "orangered", size = rank)) +
geom_point()
lookout_leaderboard %>%
ggplot(aes(x = rank, y = elapsed_time, alpha = 0.5, color = "orangered")) +
labs(title = "Lookout Mountain",
subtitle = " KOM rank vs time",
x = "KOM ranking",
y = "time (seconds)") +
geom_point()
lookout_leaderboard %>%
ggplot(aes(x = rank, y = elapsed_time, alpha = 0.5, color = "orangered"), show.legend = FALSE) +
labs(title = "Lookout Mountain",
subtitle = "Strava KOM rank vs time",
x = "KOM ranking",
y = "time (seconds)") +
geom_point()
lookout_leaderboard %>%
ggplot(aes(x = rank, y = elapsed_time, alpha = 0.5, color = "orangered")) +
labs(title = "Lookout Mountain",
subtitle = "Strava KOM rank vs time",
x = "KOM ranking",
y = "time (seconds)") +
geom_point(show.legend = FALSE)
lookout_leaderboard %>%
ggplot(aes(x = rank, y = elapsed_time, alpha = 0.5, color = "orangered")) +
labs(title = "Lookout Mountain",
subtitle = "Strava KOM rank vs time",
x = "KOM ranking",
y = "time (seconds)") +
geom_point(show.legend = FALSE)
